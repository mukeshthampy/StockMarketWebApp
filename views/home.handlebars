<br>{{posted_stuff}}<br />

<div class="jumbotron">
    <h1 class="display-4">{{stock.companyName}} ({{stock.symbol}})</h1>

    <strong>Latest Price : </strong>{{stock.latestPrice}} <br />
    <strong>Open : </strong>{{stock.open}} <br />
    <strong>High : </strong>{{stock.high}} <br />
    <strong>Low : </strong>{{stock.low}} <br />
    <strong>Clos : </strong>{{stock.close}} <br />
    <strong>Latest Volium : </strong>{{stock.latestVolume}} <br />
    <strong>Previous Close : </strong>{{stock.previousClose}} <br />
    <strong>Change : </strong>{{stock.change}} <br />
    <strong>Percentage Change : </strong>{{stock.changePercent }} <br />
    <strong>52 Week High : </strong>{{stock.week52High}} <br />
    <strong>52 Week Low : </strong>{{stock.week52Low}} <br />

</div>


